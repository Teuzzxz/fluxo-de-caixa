{
   "name": "Backroom",
   "version": "2.7.0",
   "main": "electron/main.mjs",
   "type": "module",
   "scripts": {
      "dev": "concurrently \"npm:dev:*\"",
      "dev:frontend": "vite",
      "build": "vite build",
      "build-electron": "electron-builder"
   },
   "dependencies": {
      "chart.js": "^4.5.1",
      "cross-env": "^10.1.0",
      "jsonwebtoken": "^9.0.2",
      "react": "^19.2.0",
      "react-chartjs-2": "^5.3.1",
      "react-dom": "^19.2.0",
      "react-router-dom": "^7.9.5",
      "recharts": "^3.4.1"
   },
   "devDependencies": {
      "@vitejs/plugin-react": "^5.1.1",
      "concurrently": "^8.2.2",
      "electron": "^32.3.3",
      "electron-builder": "^24.13.3",
      "vite": "^5.3.3",
      "wait-on": "^9.0.3"
   },
   "build": {
      "appId": "com.backroom.00",
      "productName": "Backroom",
      "directories": {
         "buildResources": "electron/build"
      },
      "win": {
         "icon": "electron/build/backroom_icon.ico"
      },
      "asar": false,
      "publish": {
         "provider": "github",
         "owner": "Teuzzxz",
         "repo": "fluxo-de-caixa"
      },
      "files": [
         "dist/**/*",
         "electron/**/*",
         "backend/**/*"
      ]
   }
}
